        <mxCell id="wXz9zKLrPtPnqNTYvRdY-12" parent="1" style="whiteSpace=wrap;html=1;align=center;" value="&lt;div align=&quot;center&quot;&gt;Secretario Virtual:&lt;/div&gt;&lt;div align=&quot;center&quot;&gt;Tools:&lt;/div&gt;&lt;div align=&quot;center&quot;&gt;get_performance_review&lt;/div&gt;&lt;div align=&quot;center&quot;&gt;&lt;br&gt;&lt;/div&gt;&lt;div align=&quot;center&quot;&gt;&lt;br&gt;&lt;/div&gt;" vertex="1">
